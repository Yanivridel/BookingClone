{
    "compilerOptions": {
        "target": "ES2020",
        "module": "NodeNext",
        "moduleResolution": "NodeNext",
        "sourceMap": true,
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "outDir": "./dist",
        "baseUrl": ".",
        "resolveJsonModule": true
    },
    "include": [
        "src/**/*.ts"
, "../Client/src/i18n.ts", "../Client/src/react-i18next.d.ts"    ],
    "exclude": [
        "node_modules"
    ]
}
